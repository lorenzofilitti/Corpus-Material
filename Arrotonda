def arrotonda(x):
    r = []
    for n in x:
        if n >= 4.5:
            r.append(5)
        if n <=4.4 and n >= 4:
            r.append(4)
        if n >= 3.5 and n <= 3.9:
            r.append(4)
        if n <= 3.4 and n >= 3:
            r.append(3)
        if n >= 2.5 and n <= 2.9:
            r.append(3)
        if n <= 2.4 and n >= 2:
            r.append(2)
        if n >= 1.5 and n <= 1.9:
            r.append(2)
        if n <= 1.5:
            r.append(1)
    return r
